{
  "name": "preact-x-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "pika-web --optimize",
    "prestart": "npm run build",
    "start": "http-serve",
    "build": "tsc -p . && npm run style",
    "style": "bundle-scss --config && sass --no-source-map ./tmp/:./dist/",
    "prebuild": "rm -r ./dist/",
    "postbuild": "rm -r ./tmp/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "preact": "^10.0.0-alpha.4"
  },
  "devDependencies": {
    "@pika/web": "^0.4.2",
    "bundle-scss": "^1.3.4",
    "http-serve": "^1.0.1",
    "sass": "^1.18.0",
    "typescript": "^3.4.3"
  },
  "@pika/web": {
    "webDependencies": [
      "preact",
      "preact/hooks"
    ]
  },
  "bundleStyle": {
    "dest": "./tmp/style.scss",
    "mask": "./src/**/*.scss"
  }
}
